# Contentstack CMS Configuration
# Get these values from your Contentstack account

# API Key - Found in Settings > Stack Details
CONTENTSTACK_API_KEY=your_api_key_here

# Delivery Token - Found in Settings > Tokens (create a new Delivery Token)
CONTENTSTACK_DELIVERY_TOKEN=your_delivery_token_here

# Environment - Usually 'production' (check Settings > Environments)
CONTENTSTACK_ENVIRONMENT=production

# Region - Options: 'US', 'EU', 'AZURE_NA', 'AZURE_EU', 'GCP_NA'
CONTENTSTACK_REGION=US

# Next.js Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
